<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40"
            fx:id="mainPane"
            fx:controller="ch.ge.ve.offlineadmin.controller.BallotDecryptionController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <top>
        <ButtonBar>
            <padding>
                <Insets bottom="10.0" left="0.0" right="0.0" top="10.0"/>
            </padding>
            <buttons>
                <Button text="%ballot_decryption.start_button.text" onAction="#startDecryption" ButtonBar.buttonData="OK_DONE"/>
            </buttons>
        </ButtonBar>
    </top>
    <center>
        <fx:include fx:id="consoleOutput" source="ConsoleOutput.fxml"/>
    </center>
</BorderPane>